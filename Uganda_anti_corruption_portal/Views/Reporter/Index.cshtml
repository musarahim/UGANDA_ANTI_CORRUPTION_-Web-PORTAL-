@model IEnumerable<Uganda_anti_corruption_portal.Models.Reporter>
@{
    ViewBag.Title = "View";
}
<div class="jumbotron backgroundcontent">
    <h2>Reporters</h2>
    <div class="row">
        <div class="col-md-2">
            <a href="@Url.Action("Administration","Home")" class="btn btn-primary">Back to Admin page</a>
        </div>

    </div>
    </p>
    <table class="table table-striped table-responsive-md table-dark">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.PhoneNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ReportCase.Title)
                </th>


            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tbody>
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.PhoneNumber)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ReportCase.Title)
                    </td>


                </tr>
            </tbody>
        }

    </table>

</div> 